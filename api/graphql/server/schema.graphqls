scalar JSON
scalar Any
scalar LogMessage


input Config {
   scope: [Any]
   ports: [Int]
   blacklist: [String]
   domains: [String]
   resolvers: [String]
   ips: [String]
   cirds: [Any]
   transformations: [Any]
   database: [Any]
   bruteForce: Boolean
   alterations: Boolean
}



type Session {
  sessionToken: ID!
}

#input SessionInput {
#  sessionToken: ID!
#}

input CreateSessionInput {
  config: Any!
}

input CreateSessionJsonInput {
  config: String!
}

type Asset {
  id: ID!
}

type Time {
  unixTime: Int!
  timeStamp: String!
}

type Subscription {
  #currentTime: Time!
  logMessages(sessionToken: String!): LogMessage
}

type Query {
  placeholder: String!
}

input CreateAssetInput {
  sessionToken: ID!
  assetName: String
  data: Any
}

type Mutation {
  createSession(input: CreateSessionInput!): Session
  createSessionFromJson(input: CreateSessionJsonInput!): Session
  createAsset(input: CreateAssetInput!): Asset
  terminateSession(sessionToken: String!): Boolean
}






